type Query{
    accountsList : [Compte],
    findAccount(id:String):Compte
}
type Mutation {
    addAccount(compte: BankAccountRequestDTO) : Compte,
    updateAccount(id:String,compte: BankAccountRequestDTO):Compte,
    deleteAccount(id:String):Boolean
}
type Compte{
    id :String,
    createDate : Float,
    balance : Float,
    currency : String,
    type:String
}
input BankAccountRequestDTO{
    balance : Float,
    currency : String,
    type:String
}